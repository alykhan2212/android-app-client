{
  "name": "@alykhan2212/server",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": "v14.16.0",
    "yarn": "1.22.11"
  },
  "scripts": {
    "heroku-prebuild": "yarn install",
    "build": "sequelize db:migrate:all && sequelize db:seed:all",
    "start": "node index.js",
    "server:up": "pm2 start ecosystem.config.js",
    "server:down": "pm2 stop ecosystem.config.js",
    "server:dev": "pm2-dev start ecosystem.config.js",
    "server:dev:watch": "pm2 start ecosystem.config.js --watch",
    "server:clear": "pm2 delete ecosystem.config.js",
    "server:logs": "pm2 logs",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:g:migration": "sequelize-cli migration:generate --name",
    "db:seeds": "sequelize-cli db:seed:all",
    "db:g:seed": "sequelize-cli seed:generate --name"
  },
  "license": "MIT",
  "dependencies": {
    "apollo-datasource": "^3.0.3",
    "apollo-server-express": "^3.1.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-resolvers": "^0.4.2",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "pm2": "^5.1.0",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "sequelize-cli": "^6.2.0"
  }
}
